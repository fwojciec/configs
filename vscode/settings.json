{
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[reason]": {
    "editor.tabSize": 2,
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[scala]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.tabSize": 2
  },
  "editor.fontFamily": "'Operator Mono Lig', 'Iosevka Nerd Font', Monaco, 'Fira Mono', 'Office Code Pro D', Menlo, 'Courier New', monospace",
  "editor.fontSize": 17,
  "editor.fontLigatures": false,
  // "workbench.colorCustomizations": {
  //   "titleBar.inactiveBackground": "#1b2b34"
  // },
  "go.useLanguageServer": true,
  "go.formatTool": "goimports",
  "go.lintTool": "golint",
  "go.vetOnSave": "package",
  // "go.lintTool": "golangci-lint",
  // "go.lintFlags": [
  //   "--fast"
  // ],
  "[go]": {
    "editor.tabSize": 8,
    // "editor.defaultFormatter": "ms-vscode.Go",
    "editor.snippetSuggestions": "top",
    "editor.formatOnSave": true,
    // "editor.codeActionsOnSave": {
    //   "source.organizeImports": true
    // }
  },
  "gopls": {
    "usePlaceholders": true,
    "completeUnimported": true,
    "deepCompletion": true
  },
  "files.eol": "\n",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.fontWeightBold": "500",
  "eslint.alwaysShowStatus": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "breadcrumbs.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.fontWeight": "400",
  "editor.letterSpacing": 0.1,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "entity.other.inherited-class",
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "files.associations": {
    "*.pgsql": "sql",
    "pylintrc": "ini"
  },
  "extensions.ignoreRecommendations": false,
  "editor.formatOnSave": true,
  "terminal.integrated.fontSize": 15,
  "languageServerHaskell.hlintOn": true,
  "languageServerHaskell.liquidOn": true,
  "languageServerHaskell.useCustomHieWrapper": true,
  "languageServerHaskell.useCustomHieWrapperPath": "hie-wrapper",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "scm.diffDecorationsGutterVisibility": "hover",
  "workbench.startupEditor": "newUntitledFile",
  "C_Cpp.updateChannel": "Insiders",
  "editor.links": false,
  "elixirLS.suggestSpecs": false,
  "emmet.includeLanguages": {
    "HTML (EEx)": "html"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "workbench.colorTheme": "Ocean Dark Extended",
  "python.linting.pylintUseMinimalCheckers": false,
  "python.formatting.autopep8Args": [
    "--max-line-length",
    "100",
    "--experimental"
  ],
  "java.semanticHighlighting.enabled": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "workbench.iconTheme": "ayu",
  "editor.columnSelection": false,
}