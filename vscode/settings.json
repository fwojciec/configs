{
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[reason]": {
    "editor.tabSize": 2,
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[scala]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.tabSize": 2
  },
  "editor.fontFamily": "'Fira Code', 'Operator Mono Lig', 'Iosevka Nerd Font', Monaco, 'Fira Mono', 'Office Code Pro D', Menlo, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "go.useLanguageServer": true,
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--config",
    "~/.golangci.yml"
  ],
  "go.lintOnSave": "package",
  "[go]": {
    "editor.tabSize": 8,
    "editor.snippetSuggestions": "top",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.fixAll": true
    }
  },
  "gopls": {
    "usePlaceholders": true,
    "completeUnimported": true,
    "deepCompletion": true,
    "analyses": {
      "fillreturns": true,
      "undeclaredname": true,
      "unusedparams": true,
      "nonewvars": true,
      "gc_details": true
    }
  },
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.fontWeightBold": "500",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "breadcrumbs.enabled": false,
  "editor.renderIndentGuides": false,
  "files.associations": {
    "*.pgsql": "sql",
    "pylintrc": "ini"
  },
  "extensions.ignoreRecommendations": false,
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "scm.diffDecorationsGutterVisibility": "hover",
  "workbench.startupEditor": "newUntitledFile",
  "editor.links": false,
  "editor.tabCompletion": "on",
  "emmet.includeLanguages": {
    "HTML (EEx)": "html"
  },
  "editor.suggestSelection": "first",
  "editor.occurrencesHighlight": false,
  "python.pythonPath": "/Users/filip/.pyenv/shims/python",
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "workbench.iconTheme": "field-lights-minimalistic",
  "editor.columnSelection": false,
  "python.languageServer": "Microsoft",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "gruvboxMaterial.italicComments": false,
  "gruvboxMaterial.darkContrast": "hard",
  "editor.renderWhitespace": "none",
