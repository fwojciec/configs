{
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[reason]": {
    "editor.tabSize": 2,
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.fontFamily": "Iosevka, 'Operator Mono Lig', Monaco, 'Fira Mono', 'Office Code Pro D', Menlo, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  // "workbench.colorCustomizations": {
  //   "titleBar.inactiveBackground": "#1b2b34"
  // },
  "go.useLanguageServer": true,
  "go.formatTool": "goimports",
  "go.lintTool": "golint",
  "go.vetOnSave": "package",
  // "go.lintTool": "golangci-lint",
  // "go.lintFlags": [
  //   "--fast"
  // ],
  "[go]": {
    "editor.tabSize": 8,
    // "editor.defaultFormatter": "ms-vscode.Go",
    "editor.snippetSuggestions": "top",
    "editor.formatOnSave": true,
    // "editor.codeActionsOnSave": {
    //   "source.organizeImports": true
    // }
  },
  "gopls": {
    "usePlaceholders": true,
    "completeUnimported": true,
    "deepCompletion": true
  },
  "files.eol": "\n",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.fontWeightBold": "500",
  "eslint.alwaysShowStatus": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "prettier.printWidth": 100,
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "breadcrumbs.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.renderIndentGuides": false,
  "editor.fontWeight": "400",
  "editor.letterSpacing": 0.1,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "entity.other.inherited-class",
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "files.associations": {
    "*.pgsql": "sql"
  },
  "extensions.ignoreRecommendations": false,
  "editor.formatOnSave": true,
  "terminal.integrated.fontSize": 15,
  "languageServerHaskell.hlintOn": true,
  "languageServerHaskell.liquidOn": true,
  "languageServerHaskell.useCustomHieWrapper": true,
  "languageServerHaskell.useCustomHieWrapperPath": "hie-wrapper",
  "editor.minimap.renderCharacters": false,
  "workbench.colorTheme": "Palenight Theme",
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "field-lights-monochrome-minimalistic",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "scm.diffDecorationsGutterVisibility": "hover"
}