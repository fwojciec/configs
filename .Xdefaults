Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

!!Theme Ten
*foreground:            #9f9f9f
*background:            #1c1c1c

!Tango
! Black
!*color0:  #2E3436
*color0:  #1F1F1F
*color8:  #555753
! Red
*color1:  #CC0000
*color9:  #EF2929
! Green
*color2:  #4E9A06
*color10: #8AE234
! Yellow
*color3:  #C4A000
*color11: #FCE94F
! Blue
*color4:  #3465A4
*color12: #729FCF
! Purple
*color5:  #75507B
*color13: #AD7FA8
! Cyan
*color6:  rgb:06/98/9A
*color14: #34E2E2
! White
!*color7:  #D3D7CF
*color7:  #9f9f9f
*color15: #e0e0e0

!!xterm settings
xterm*dynamicColors:      true
xterm*utf8:			          2
xterm*termName:           xterm-256color
xterm*eightBitInput:	    true
xterm*saveLines:		      32767
xterm*scrollTtyKeypress:  true
xterm*scrollTtyOutput:	  false
xterm*scrollBar:		      false
xterm*faceName:			      Envy Code R:size=10
!xterm*faceName:			      Dina:size=10
xterm*jumpScroll:		      true
xterm*multiScroll:		    true
xterm*toolBar:			      false
xterm*geometry:			      120x30

!!urxvt settings
urxvt*termName:			      rxvt-256color
!urxvt*termName:			      rxvt-unicode
urxvt*depth:			        24
!urxvt*font:		            xft:Envy Code R:size=10
!urxvt*font:		          xft:Dina:size=8
urxvt*font:			        -*-dina-medium-r-*-*-*-*-*-*-*-*-*-*
urxvt*boldFont:          -*-dina-bold-r-*-*-*-*-*-*-*-*-*-*
urxvt*italicFont:			  -*-dina-medium-i-*-*-*-*-*-*-*-*-*-*
urxvt*boldItalicFont:	  -*-dina-bold-i-*-*-*-*-*-*-*-*-*-*
urxvt*internalBorder:     0
URxvt*borderLess:         false 
urxvt*cursorColor:		    #8f8f8f
urxvt*pointerColor:		    #8f8f8f
urxvt.colorIT:            #8f8f8f
urxvt.colorBD:            #8f8f8f
urxvt*saveLines:		      32767
urxvt*scrollBar:		      false
urxvt*cursorBlink:		    false
urxvt*geometry:			      110x28
urxvt*perl-lib:			      /usr/lib/urxvt/perl/
urxvt*perl-ext-common:	  default,matcher,searchable-scrollback
urxvt*urlLauncher:		    /usr/bin/firefox
urxvt*matcher.button:	    1
urxvt*externalBorder:     0
urxvt*title:			        urxvt
urxvt*scrollTtyOutput:	  false
urxvt*scrollTtyKeypress:	true
urxvt*scrollWithBuffer:   true 
urxvt*secondaryScroll:	  true
urxvt*secondaryScreen:    false
urxvt*print-pipe:         cat > $(echo $HOME/tmp/urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*loginShell:         true
!urxvt*fading:             25%

